# Terraform shared configuration for {{ target_vm }} VM group
# Generated by Ansible - contains provider configuration only
# This file should only be generated once per VM group

terraform {
  required_providers {
    libvirt = {
      source = "dmacvicar/libvirt"
    }
  }
}

provider "libvirt" {
  uri = "{{ libvirt_uri | default('qemu:///session') }}"
}
